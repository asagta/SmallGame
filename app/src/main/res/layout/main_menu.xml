<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/cl">

    <ImageSwitcher
        android:id="@+id/imageSwitcher"
        android:layout_width="236dp"
        android:layout_height="138dp"
        android:layout_marginLeft="-556dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="110dp"
        android:layout_height="41dp"
        android:text="TextView"
        android:layout_marginTop="31dp"
        app:layout_constraintTop_toBottomOf="@+id/imageSwitcher"
        app:layout_constraintLeft_toRightOf="@+id/button29"
        android:layout_marginLeft="0dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="110dp"
        android:layout_height="41dp"
        android:layout_marginTop="32dp"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@+id/imageSwitcher2"
        app:layout_constraintRight_toLeftOf="@+id/button31"
        android:layout_marginRight="0dp" />

    <Button
        android:id="@+id/button28"
        android:layout_width="72dp"
        android:layout_height="42dp"
        android:background="@drawable/nxt"
        android:layout_marginTop="31dp"
        app:layout_constraintTop_toBottomOf="@+id/imageSwitcher"
        app:layout_constraintLeft_toRightOf="@+id/textView10"
        android:layout_marginLeft="0dp" />

    <Button
        android:id="@+id/button29"
        android:layout_width="73dp"
        android:layout_height="42dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="31dp"
        android:background="@drawable/prv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageSwitcher" />

    <Button
        android:id="@+id/button30"
        android:layout_width="72dp"
        android:layout_height="42dp"
        android:layout_marginRight="-1dp"
        android:layout_marginTop="31dp"
        android:background="@drawable/prv"
        app:layout_constraintRight_toLeftOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/imageSwitcher2" />

    <Button
        android:id="@+id/button31"
        android:layout_width="72dp"
        android:layout_height="42dp"
        android:layout_marginRight="25dp"
        android:layout_marginTop="31dp"
        android:background="@drawable/nxt"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageSwitcher2" />

    <Button
        android:id="@+id/button32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-3dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/go"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.481" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="65dp"
        android:layout_height="22dp"
        android:text="OVERS"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@+id/imageSwitcher"
        android:layout_marginLeft="15dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <TextView
        android:id="@+id/t20"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:text="20"
        android:textSize="25dp"
        android:textStyle="bold"
        android:background="#0000ff"
        android:textColor="@color/white"
        app:layout_constraintLeft_toRightOf="@+id/imageSwitcher"
        android:layout_marginLeft="7dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/t50"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:text="50"
        android:textSize="25dp"
        android:textStyle="bold"
        android:background="#0000ff"
        android:textColor="@color/white"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintRight_toLeftOf="@+id/imageSwitcher2"
        android:layout_marginRight="14dp" />

    <ImageSwitcher
        android:id="@+id/imageSwitcher2"
        android:layout_width="234dp"
        android:layout_height="140dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toRightOf="@+id/imageSwitcher"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>