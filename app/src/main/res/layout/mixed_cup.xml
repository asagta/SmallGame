<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mtrophy_list"
    android:id="@+id/constr">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="177dp"
        android:layout_height="129dp"
        app:srcCompat="@drawable/eng_logos"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="149dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="177dp"
        android:layout_height="129dp"
        app:srcCompat="@drawable/aus_logos"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="149dp" />

    <Button
        android:id="@+id/button37"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="143dp"
        android:layout_height="48dp"
        android:layout_marginBottom="56dp"
        android:layout_marginLeft="16dp"
        android:background="@drawable/rounded_square"
        android:elevation="17dp"
        android:text="START"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="539dp"
        android:layout_height="31dp"
        android:text="Super10 : T20 Match"
        android:textSize="30dp"
        android:textColor="@color/black"
        android:gravity="left|center_horizontal"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button38"
        android:layout_width="143dp"
        android:layout_height="48dp"
        android:background="@drawable/rounded_square"
        android:elevation="17dp"
        android:text="FIXTURES"
        android:textColor="@color/black"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="-3dp" />

    <Button
        android:id="@+id/button39"
        android:layout_width="143dp"
        android:layout_height="48dp"
        android:background="@drawable/rounded_square"
        android:elevation="17dp"
        android:text="STANDINGS"
        android:textColor="@color/black"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="56dp"
        android:layout_marginRight="16dp" />

    <Button
        android:id="@+id/button40"
        android:layout_width="143dp"
        android:layout_height="48dp"
        android:background="@drawable/rounded_square"
        android:elevation="17dp"
        android:text="STATS"
        android:textColor="@color/black"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="-3dp"
        android:layout_marginRight="16dp" />
</android.support.constraint.ConstraintLayout>